    "babel-runtime": "^6.25.0",
    "throttle-debounce": "^1.0.1"
  },
  "devDependencies": {
    "prop-types": "^15.6.0",
    "@storybook/addon-actions": "3.3.4",
    "@storybook/react": "3.3.4",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
